{
  "namespace": "redstone",
  
  "crafter_input_grid": {
    "type": "grid",
    "anchor_from": "center",
    "anchor_to": "center",
    "offset": "$grid_offset",
    "grid_dimensions": "$grid_dimensions",
    "collection_name": "container_items",
    //"grid_item_template": "redstone.crafter_enabled_slot_template",
    //"$item_collection_name": "container_items",
    
    // From "inventory_screen"
    "controls": [
      {
        "crafting_input_item_top_left@redstone.crafter_enabled_slot_template1": {
          "grid_position": [ 0, 0 ],

          "$focus_id_binding_type": "none",
          "$focus_id": "crafting_input_top_left",

          "$focus_override_left_binding_type": "none",
          "$focus_override_left": "armor_item_head"
        }
      },
      {
        "crafting_input_item_top_mid@redstone.crafter_enabled_slot_template2": {
          "grid_position": [ 1, 0 ]
        }
      },
      {
        "crafting_input_item_top_right@redstone.crafter_enabled_slot_template3": {
          "grid_position": [ 2, 0 ]
        }
      },
      {
        "crafting_input_item_mid_left@redstone.crafter_enabled_slot_template4": {
          "grid_position": [ 0, 1 ],

          "$focus_id_binding_type": "none",
          "$focus_id": "crafting_input_mid_left",

          "$focus_override_left_binding_type": "none",
          "$focus_override_left": "armor_item_legs"
        }
      },
      {
        "crafting_input_item_middle@redstone.crafter_enabled_slot_template5": {
          "grid_position": [ 1, 1 ]
        }
      },
      {
        "crafting_input_item_mid_right@redstone.crafter_enabled_slot_template6": {
          "grid_position": [ 2, 1 ]
        }
      },
      {
        "crafting_input_item_bot_left@redstone.crafter_enabled_slot_template7": {
          "grid_position": [ 0, 2 ]
        }
      },
      {
        "crafting_input_item_bot_mid@redstone.crafter_enabled_slot_template8": {
          "grid_position": [ 1, 2 ]
        }
      },
      {
        "crafting_input_item_bot_right@redstone.crafter_enabled_slot_template9": {
          "grid_position": [ 2, 2 ],

          "$focus_override_right_binding_type": "none",
          "$focus_override_right": "crafting_output_item"
        }
      }
    ]
  },
